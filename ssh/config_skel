# -----------------------------------------------------------------------------
# ~zozo/.ssh/config_skel
# vim:ft=conf:fo-=a:fo-=t:
# -----------------------------------------------------------------------------



# -----------------------------------------------------------------------------
# Back to My Mac
# -----------------------------------------------------------------------------

Host *.BTMM
    AddressFamily inet6
    Protocol 2

# -----------------------------------------------------------------------------
# general
# -----------------------------------------------------------------------------

Host *
    User zozo
    SendEnv LANG LC_* TZ GIT_* TERM_PROGRAM solarized
    HashKnownHosts yes
    #VisualHostKey yes
    Compression yes
    ForwardAgent yes
    ConnectionAttempts 5
    ControlMaster auto
    ControlPath ~/.ssh/%r@%h:%p

    #ProxyCommand nc -X5 -x 127.0.0.1:8080 %h %p
    #ProxyCommand socat - SOCKS:127.0.0.1:%h:%p,socksport=8080
