{
  "autoSnippet.snippets": [
    {
      "language": "fish",
      "snippet": "fish_function"
    },
    {
      "language": "gemfile",
      "snippet": "new_Gemfile"
    },
    {
      "language": "json",
      "snippet": "new_JSON"
    },
    {
      "language": "markdown",
      "snippet": "YAML_frontmatter"
