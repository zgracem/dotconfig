  "yaml.format.enable": false,
  "yaml.format.proseWrap": "always",
  "yaml.maxItemsComputed": 20480,
  "yaml.schemaStore.enable": false,
  "yard.tags.paramNameBeforeType": true,
  "[applescript]": {
