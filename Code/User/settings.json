  "settingsSync.keybindingsPerPlatform": false,
  "shellcheck.enableQuickFix": true,
  "shellcheck.exclude": ["1090", "1091"],
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "shellcheck.run": "onSave",
  "shellcheck.useWorkspaceRootAsCwd": true,
  "shellformat.flag": "--func-next-line --indent=2 --case-indent --binary-next-line",
  "shellformat.path": "/usr/local/bin/shfmt",
  "slimLint.executablePath": "/Users/zozo/.local/share/rbenv/shims/slim-lint",
  "solargraph.bundlerPath": "/Users/zozo/.local/share/rbenv/shims/bundle",
  "solargraph.commandPath": "/Users/zozo/.local/share/rbenv/shims/solargraph",
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "sortJSON.contextMenu": {
    "sortJSONAlphaNum": false,
    "sortJSONAlphaNumReverse": false,
    "sortJSONKeyLength": false,
    "sortJSONKeyLengthReverse": false,
    "sortJSONReverse": false,
    "sortJSONTypeReverse": false,
    "sortJSONValuesReverse": false
