{
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "code-runner.clearPreviousOutput": true,
  "code-runner.customCommand": "echo $PATH",
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "applescript": "osascript -ss",
    "erb": "erb -T-",
    "fish": "env TERM=dumb /usr/local/bin/fish",
    "html": "links -html-margin 0 -dump",
    "jq": "jq --null-input --from-file",
    "makefile": "cd $dir && gnumake -n",
    "markdown": "multimarkdown --snippet --nolabels",
    "ruby": "ruby",
    "sass": "sass --no-source-map --trace",
    "scss": "sass --no-source-map --trace",
    "sed": "pbpaste | gsed -E -f",
    "slim": "slimrb --option format=:html --option tabsize=2 --option sort_attrs=false --pretty --trace",
    "troff": "man -Pcat"
  },
  "code-runner.executorMapByGlob": {
    "*.[1-9].md": "ronn --pipe < $fullFileName | man -Pcat -Eutf8 -l -",
    "*.ti": "tic -Lxfq $fullFileName"
  },
  "code-runner.fileDirectoryAsCwd": true,
  "code-runner.ignoreSelection": true,
  "code-runner.runInTerminal": false,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.showRunCommandInEditorContextMenu": false,
  "code-runner.showRunCommandInExplorerContextMenu": false,
  "code-runner.showRunIconInEditorTitleMenu": false,
  "code-runner.temporaryFileName": "",
  "debug.showInlineBreakpointCandidates": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.accessibilitySupport": "off",
  "editor.colorDecorators": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorStyle": "line-thin",
  "editor.find.seedSearchStringFromSelection": "never",
  "editor.foldingHighlight": false,
  "editor.fontFamily": "IBM Plex Mono, Office Code Pro, Consolas, monospace",
  "editor.fontSize": 14,
  "editor.fontWeight": "300",
  "editor.formatOnSaveMode": "modifications",
  "editor.guides.bracketPairs": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.delay": 200,
  "editor.insertSpaces": true,
  "editor.lightbulb.enabled": false,
  "editor.links": false,
  "editor.maxTokenizationLineLength": 40960,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "other": "on",
    "strings": "on"
  },
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 118],
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showSnippets": true,
  "editor.suggestOnTriggerCharacters": false,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "onlySnippets",
  "editor.unicodeHighlight.allowedCharacters": {
    "×": true,
    "‘": true,
    "’": true,
    "“": true,
    "”": true
  },
  "editor.unicodeHighlight.includeComments": false,
  "editor.unicodeHighlight.includeStrings": false,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 118,
  "editor.wrappingStrategy": "advanced",
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.m4": "${capture}",
    "*.sass": "${capture}.css",
    "*.scss": "${capture}.css",
    "*.slim": "${capture}.html"
  },
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.visible": 0,
  "extensions.closeExtensionDetailsOnViewChange": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.JSON-tmLanguage": "json",
    "*.YAML-tmLanguage": "yaml",
    "*.[1-9]": "troff",
    "*.cscollection": "json",
    "*.csv": "csv",
    "*.geojson": "json",
    "*.make": "makefile",
    "*.plist": "xml",
    "*.sublime-syntax": "yaml",
    "*.sublime-{settings,build,keymap}": "json",
    "*ignore": "ignore"
  },
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.eol": "\n",
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autoRepositoryDetection": "subFolders",
  "git.defaultCloneDirectory": "/Users/zozo/src/github.com",
  "git.openDiffOnClick": false,
  "git.openRepositoryInParentFolders": "never",
  "git.path": "/usr/local/bin/git",
  "git.repositoryScanMaxDepth": -1,
  "gitlens.advanced.blame.customArguments": ["-w"],
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.blame.avatars": false,
  "gitlens.blame.heatmap.location": "left",
  "gitlens.changes.locations": ["overview", "gutter"],
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": true,
