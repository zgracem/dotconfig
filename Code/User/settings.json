{
  "accessibility.signals.terminalBell": {
    "sound": "off"
  },
  "chat.commandCenter.enabled": false,
  "code-runner.clearPreviousOutput": true,
  "code-runner.customCommand": "echo $PATH",
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "applescript": "osascript -ss",
    "erb": "erb -T-",
    "fish": "env TERM=dumb /usr/local/bin/fish",
    "html": "links -html-margin 0 -dump",
    "jq": "jq --null-input --from-file",
    "makefile": "cd $dir && gnumake -n",
    "markdown": "multimarkdown --snippet --nolabels",
    "powershell": "pwsh -ExecutionPolicy ByPass -File",
    "python": "python3",
    "ruby": "/usr/local/bin/ruby",
    "sass": "sass --no-source-map --trace",
    "scss": "sass --no-source-map --trace",
    "sed": "pbpaste | gsed -E -f",
    "shellscript": "/usr/local/bin/bash",
    "troff": "man -Pcat"
  },
  "code-runner.executorMapByGlob": {
    "*.[1-9].md": "ronn --pipe < $fullFileName | man -Pcat -Eutf8 -l -",
    "*.html.slim": "slimrb --option format=:html --option tabsize=2 --option sort_attrs=false --pretty --trace",
    "*.ti": "tic -Lxfq $fullFileName",
    "*.xml.slim": "slimrb --option format=:xml --option tabsize=2 --option sort_attrs=false --pretty --trace"
  },
  "code-runner.fileDirectoryAsCwd": true,
  "code-runner.ignoreSelection": true,
  "code-runner.respectShebang": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.showRunCommandInEditorContextMenu": false,
  "code-runner.showRunCommandInExplorerContextMenu": false,
  "code-runner.showRunIconInEditorTitleMenu": false,
  "debug.showInlineBreakpointCandidates": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.accessibilitySupport": "off",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.experimentalWhitespaceRendering": "off",
  "editor.find.seedSearchStringFromSelection": "never",
  "editor.foldingHighlight": false,
  "editor.fontFamily": "IBM Plex Mono, Office Code Pro, Consolas, monospace",
  "editor.fontSize": 13,
  "editor.fontWeight": "300",
  "editor.formatOnSaveMode": "modifications",
  "editor.guides.bracketPairs": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.insertSpaces": true,
  "editor.lightbulb.enabled": "off",
  "editor.links": false,
