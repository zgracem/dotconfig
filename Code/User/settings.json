  "gitlens.heatmap.toggleMode": "file",
  "gitlens.hovers.annotations.enabled": true,
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.menus": false,
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.sortBranchesBy": "date:desc",
  "gitlens.statusBar.format": "${author, }${agoOrDate}${ (pullRequest)} · ${message}",
  "gitlens.views.branches.showBranchComparison": false,
  "html.format.indentInnerHtml": true,
  "html.format.wrapLineLength": 0,
  "insertGuid.showCodeSnippets": false,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "jqPlayground.binaryPath": "/usr/local/bin/jq",
  "json.maxItemsComputed": 20480,
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.list.indentationSize": "inherit",
  "markdown.extension.toc.levels": "1..3",
  "markdown.extension.toc.orderedList": true,
  "minify.css": {
    "roundingPrecision": -1
  },
  "minify.js": {
    "compress": {
      "collapse_vars": true,
      "passes": 2,
      "reduce_vars": true
    }
  },
  "oneDarkPro.editorTheme": "One Dark Pro Flat",
  "oneDarkPro.italic": true,
  "outline.problems.enabled": false,
  "prettier.bracketSameLine": true,
  "prettier.proseWrap": "preserve",
  "prettier.quoteProps": "consistent",
  "prettier.semi": false,
  "prettier.trailingComma": "none",
  "problems.showCurrentInStatus": true,
  "redhat.telemetry.enabled": false,
  "rewrap.wholeComment": false,
  "ruby.interpreter.commandPath": "/Users/zozo/.local/share/rbenv/shims/ruby",
  "ruby.pathToBundler": "/Users/zozo/.local/share/rbenv/shims/bundle",
  "ruby.useLanguageServer": false,
  "scm.defaultViewMode": "tree",
  "search.collapseResults": "auto",
  "search.searchEditor.defaultNumberOfContextLines": 2,
  "search.searchEditor.doubleClickBehaviour": "openLocationToSide",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.sortOrder": "default",
  "security.workspace.trust.enabled": false,
  "settingsSync.ignoredExtensions": ["castwide.solargraph"],
  "settingsSync.ignoredSettings": [
    "code-runner.customCommand",
    "code-runner.executorMap",
    "code-runner.executorMapByGlob",
    "code-runner.terminalRoot",
    "editor.fontFamily",
    "editor.fontSize",
    "editor.fontWeight",
    "jqPlayground.binaryPath",
    "papyrus.fo4.directory",
    "slimLint.executablePath",
    "solargraph.bundlerPath",
    "solargraph.commandPath",
    "solargraph.diagnostics",
    "solargraph.formatting",
    "spellright.configurationScope",
    "spellright.language",
    "spellright.addToSystemDictionary",
    "window.doubleClickIconToClose"
