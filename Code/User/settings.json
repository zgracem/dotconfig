  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "code-runner.clearPreviousOutput": true,
  "code-runner.customCommand": "echo $PATH",
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "fish": "fish -C 'set -gx TERM dumb'",
    "html": "links -html-margin 0 -dump",
    "jq": "jq --null-input --from-file",
    "markdown": "multimarkdown --snippet --nolabels",
    "ruby": "ruby -w",
    "sass": "sass --no-source-map --style=expanded --trace",
    "scss": "sass --no-source-map --style=expanded --trace",
    "slim": "slimrb --option format=:html --option tabsize=2 --pretty --trace",
    "troff": "man -Pcat"
  },
  "code-runner.executorMapByGlob": {
    "*.[1-9].md": "ronn --pipe < $fullFileName | man -Pcat -Eutf8 -l -",
    "*.ti": "tic -Lxfq $fullFileName"
  },
  "code-runner.ignoreSelection": true,
  "code-runner.runInTerminal": false,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.showRunCommandInEditorContextMenu": false,
  "code-runner.showRunCommandInExplorerContextMenu": false,
  "code-runner.showRunIconInEditorTitleMenu": false,
  "code-runner.temporaryFileName": "",
  "debug.showInlineBreakpointCandidates": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.colorDecorators": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorStyle": "line-thin",
  "editor.find.seedSearchStringFromSelection": false,
  "editor.foldingHighlight": false,
  "editor.fontFamily": "IBM Plex Mono, Office Code Pro, Consolas, monospace",
  "editor.fontSize": 12,
  "editor.fontWeight": "300",
  "editor.formatOnSaveMode": "modifications",
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.delay": 120,
  "editor.lightbulb.enabled": false,
  "editor.links": false,
  "editor.minimap.renderCharacters": false,
