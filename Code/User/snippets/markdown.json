{
  "Heading_1": {
    "prefix": "h1",
    "body": ["${1:Heading 1}", "${1/./=/g}\n", "$0"],
    "description": "Heading 1 (Setext)"
  },
  "Heading_2": {
    "prefix": "h2",
    "body": ["${1:Heading 2}", "${1/./-/g}\n", "$0"],
    "description": "Heading 2 (Setext)"
  },
  "inline_link": {
    "prefix": "hhref",
    "body": "[$TM_SELECTED_TEXT](${CLIPBOARD/.*(https?:\\/\\/[^\\s\"']+).*/${1}/})",
    "description": "Inline link (^L)"
  }
}
