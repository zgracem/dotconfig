[user]
	name = Zozo
	email = zozo@inescapable.org

[core]
	autocrlf = input
	safecrlf = true
	excludesfile = ~/.config/git/globalignore
	filemode = false

[alias]
	# http://gitimmersion.com/lab_11.html
	co = checkout
	cm = commit
	st = status
	br = branch
	df = diff
	hist = log --pretty=format:'[%h] %ad | %s%d' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	unstage = reset HEAD

[hub]
	protocol = https

[color]
	ui = true
	branch = true
	diff = true
	interactive = true
	status = true
